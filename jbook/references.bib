	keywords = {Computer Science - Computer Vision and Pattern Recognition},
	file = {Cross-view Relation Networks for Mammogram Mass Detection.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\Cross-view Relation Networks for Mammogram Mass Detection.md:text/plain;Cross-view Relation Networks for Mammogram Mass Detection.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\Cross-view Relation Networks for Mammogram Mass Detection.md:text/plain;Ma et al. - 2019 - Cross-view Relation Networks for Mammogram Mass De.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\QHK8G6FS\\Ma et al. - 2019 - Cross-view Relation Networks for Mammogram Mass De.pdf:application/pdf},
