	year = {2021},
	keywords = {Preprocessing, BCD Screen I - Include},
	pages = {33438--33463},
	file = {Beeravolu et al. - 2021 - Preprocessing of Breast Cancer Images to Create Da.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\2RUIVMMR\\Beeravolu et al. - 2021 - Preprocessing of Breast Cancer Images to Create Da.pdf:application/pdf;Preprocessing of Breast Cancer Images to Create Datasets for Deep-CNN.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\Preprocessing of Breast Cancer Images to Create Datasets for Deep-CNN.md:text/plain},
