	file = {Khan et al. - 2003 - An Optimal Multivariate Stratified Sampling Design.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\7XD3EVA5\\Khan et al. - 2003 - An Optimal Multivariate Stratified Sampling Design.pdf:application/pdf},
