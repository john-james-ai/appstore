   "source": [
    "from appstore.container import AppstoreContainer\n",
    "container = AppstoreContainer()\n",
    "container.init_resources()\n",
    "container.wire(packages=[\"appstore.data.acquisition\"])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "uow = container.data.uow()\n",
    "uow.rating_repo.reset()"
   ]
  }
